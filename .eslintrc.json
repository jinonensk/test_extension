{
  "root": true,
  "env": {
    "node": true,
    "webextensions": true
  },
  "extends": [
    "plugin:vue/recommended",
    "@vue/airbnb"
  ],
  "rules": {
    "no-plusplus": 0,
    "no-restricted-globals": 0,
    "no-loop-func": 0,
    "vue/html-self-closing": 0
  },
  "parserOptions": {
    "parser": "babel-eslint"
  }
}